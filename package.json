{
  "name": "fetch-decorators",
  "version": "0.0.0",
  "description": "A set of ES7 composable decorators around the fetch api",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src/*.js test/*.js",
    "pretest": "npm run lint",
    "test": "babel-node `npm bin`/isparta cover --include 'src/*' --report lcov --report html --report text ./test/index.js | colortape",
    "report-coverage": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "github.com/ArnaudRinquin/fetch-decorators"
  },
  "keywords": [
    "fetch",
    "decorators",
    "es7",
    "es2016"
  ],
  "author": "Arnaud Rinquin",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-eslint": "^4.1.3",
    "codeclimate-test-reporter": "^0.1.1",
    "colortape": "^0.1.1",
    "cz-conventional-changelog": "^1.1.4",
    "eslint": "^1.7.3",
    "fetch-mock": "^2.2.3",
    "isparta": "^3.1.0",
    "tape": "^4.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
